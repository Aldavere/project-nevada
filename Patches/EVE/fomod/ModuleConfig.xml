<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://qconsulting.ca/fo3/ModConfig5.0.xsd">
	<moduleName>Project Nevada - EVE Support</moduleName>
	<moduleDependencies operator="And">
		<dependencies operator="Or">
			<fileDependency file="Project Nevada - Core.esm" state="Active"/>
			<fileDependency file="Project Nevada - Core.esm" state="Inactive"/>
		</dependencies>
		<dependencies operator="Or">
			<fileDependency file="Project Nevada - Equipment.esm" state="Active"/>
			<fileDependency file="Project Nevada - Equipment.esm" state="Inactive"/>
		</dependencies>
		<dependencies operator="Or">
			<fileDependency file="EVE FNV - ALL DLC.esp" state="Active"/>
			<fileDependency file="EVE FNV - ALL DLC.esp" state="Inactive"/>
			<fileDependency file="EVE FNV - NO DLC.esp" state="Active"/>
			<fileDependency file="EVE FNV - NO DLC.esp" state="Inactive"/>
			<fileDependency file="EVE FNV - NO GRA.esp" state="Active"/>
			<fileDependency file="EVE FNV - NO GRA.esp" state="Inactive"/>
		</dependencies>
	</moduleDependencies>
	<requiredInstallFiles>
		<folder source="meshes"/>
		<folder source="textures"/>
	</requiredInstallFiles>
	<conditionalFileInstalls>
		<patterns>
			<pattern>
				<dependencies operator="Or">
					<fileDependency file="EVE FNV - ALL DLC.esp" state="Active"/>
					<fileDependency file="EVE FNV - ALL DLC.esp" state="Inactive"/>
				</dependencies>
				<files>
					<file source="Project Nevada - EVE All DLC.esp"/>
				</files>
			</pattern>
			<pattern>
				<dependencies operator="Or">
					<fileDependency file="EVE FNV - NO DLC.esp" state="Active"/>
					<fileDependency file="EVE FNV - NO DLC.esp" state="Inactive"/>
				</dependencies>
				<files>
					<file source="Project Nevada - EVE No DLC.esp"/>
				</files>
			</pattern>
			<pattern>
				<dependencies operator="Or">
					<fileDependency file="EVE FNV - NO GRA.esp" state="Active"/>
					<fileDependency file="EVE FNV - NO GRA.esp" state="Inactive"/>
				</dependencies>
				<files>
					<file source="Project Nevada - EVE No GRA.esp"/>
				</files>
			</pattern>
		</patterns>
	</conditionalFileInstalls>
</config>